        <code class=3D"n">result</code> <code class=3D"o">=3D</code> <code =
class=3D"n">func</code><code class=3D"p">(</code><code class=3D"o">*</code>=
<code class=3D"n">args</code><code class=3D"p">,</code> <code class=3D"o">*=
*</code><code class=3D"n">kwargs</code><code class=3D"p">)</code>
        <code class=3D"n">elapsed</code> <code class=3D"o">=3D</code> <code=
 class=3D"n">time</code><code class=3D"o">.</code><code class=3D"n">perf_co=
unter</code><code class=3D"p">()</code> <code class=3D"o">-</code> <code cl=
ass=3D"n">t0</code>
        <code class=3D"n">name</code> <code class=3D"o">=3D</code> <code cl=
ass=3D"n">func</code><code class=3D"o">.</code><code class=3D"nv-Magic">__n=
ame__</code>
        <code class=3D"n">arg_lst</code> <code class=3D"o">=3D</code> <code=
 class=3D"p">[]</code>
        <code class=3D"k">if</code> <code class=3D"n">args</code><code clas=
s=3D"p">:</code>
